<!DOCTYPE html>
<html>
<head>
  <title>Witter</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">

    <div class="row">
      <header>
        <h1><%= link_to 'Witter', root_path %></h1>
        <%= link_to "New Wit", new_wit_path %>
      </header>
    </div>

    <div class="row">
      <div class="content">
        <%= yield %>
      </div>
    </div>


    <div class="row">
      <footer>
        <p>Made with &hearts;</p>
      </footer>
    </div>

  </div>
</body>
</html>
