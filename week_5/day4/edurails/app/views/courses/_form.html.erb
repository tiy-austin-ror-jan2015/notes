<%= simple_form_for(@course) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :location_id, collection: location_collection %>
    <%= f.input :title %>
    <%= f.input :reqs %>
    <%= f.input :enrollment_cap %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
