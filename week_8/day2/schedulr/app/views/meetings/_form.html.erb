<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.association :user %>
    <%= f.association :room %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
