<h1><%= title %></h1>
<hr>
<table class='table table-striped table-bordered table-hover'>
  <thead>
    <tr>
      <% type.report_columns.each do |attr| %>
        <th><%= attr.titleize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
  <% records.each do |pet| %>
    <tr>
      <% pet.report_values.each do |val| %>
        <td><%= val %></td>
      <% end %>
    </tr>
  <% end %>
</tbody>
</table>
